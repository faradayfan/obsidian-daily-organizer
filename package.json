{
	"name": "obsidian-daily-organizer",
	"version": "1.1.1",
	"description": "Automatically migrate todos, add task metadata with natural language parsing, tag tasks by project, and generate AI-powered project updates for your Obsidian daily notes",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint .",
		"prepare-release": "npm run lint && npm test && npm run build"
	},
	"keywords": [
		"obsidian",
		"obsidian-plugin",
		"daily-notes",
		"todo",
		"task-management",
		"ai",
		"llm",
		"project-management",
		"automation",
		"productivity"
	],
	"author": "faradayfan",
	"repository": {
		"type": "git",
		"url": "https://github.com/faradayfan/obsidian-daily-organizer.git"
	},
	"bugs": {
		"url": "https://github.com/faradayfan/obsidian-daily-organizer/issues"
	},
	"homepage": "https://github.com/faradayfan/obsidian-daily-organizer#readme",
	"license": "0-BSD",
	"devDependencies": {
		"@eslint/js": "9.30.1",
		"@types/jest": "^29.5.14",
		"@types/node": "^25.2.2",
		"esbuild": "0.27.3",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"jest": "^29.7.0",
		"jiti": "2.6.1",
		"ts-jest": "^29.4.6",
		"tslib": "2.8.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.0"
	},
	"dependencies": {
		"obsidian": "^1.11.4"
	}
}
